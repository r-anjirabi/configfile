<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>ConfigFile</id>
        <version>1.0.9</version>
        <title>ConfigFile</title>
        <authors>virtualdreams</authors>
        <owners>virtualdreams</owners>
        <projectUrl>https://github.com/virtualdreams/ConfigFile</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>
			Text based configuration files based on key value pairs. Support for:
			- Singleline values
			- Multiline values
			- Here document mode
			- Literal mode
			- Events for add and change key
		</description>
        <tags>config, text, settings,</tags>
